  #pro_tips .vidFilter a.active{pointer-events:none}
  #pro_tips .grid-number{position:relative;margin-bottom:10px}
  #pro_tips .grid-number .vidOverlay{
  	position:absolute;
    top:0;
    left:0;
    width:100%;
    height:100%;
    z-index:9;
    cursor:pointer;
    text-align:center
  }
  #pro_tips .vidOverlay .img{
  	position:absolute;
    top:0;
    left:0;
    width:100%;
    height:100%;
    background-size:cover;
    background-position:center;
    z-index:2
  }
  #pro_tips .vidOverlay .img:before{
  	content:"";
    position:absolute;
    top:0;
    left:0;
    width:100%;
    height:100%;
    background:rgba(255, 255, 255, 0.82)
  }
  #pro_tips .vidOverlay h4{
  	font-size:36px;
    line-height:1.1;
    color:#F98900;
    font-weight:bold;
    z-index:9;
    opacity:1 !important;
    position:absolute;
    width:100%;
    top:50%;
    transform:translateY(-50%);
    text-transform:uppercase;
  }
  #pro_tips .vidOverlay:hover .img:before{
  	background:rgba(255, 255, 255, 0);
  }
  #pro_tips .vidOverlay:hover h4{
  	text-shadow:1px 1px 5px #333;
  }
  
