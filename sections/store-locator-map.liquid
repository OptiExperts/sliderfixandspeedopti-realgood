     padding:0 !important 
}
#find-a-store-near-you-below .create-custom-page{
    padding-bottom:0
}
  
@media screen and (min-width:768px){
     #storemapper #storemapper-form{
         position:absolute;
         top:10px;
         right:10px;
         width: auto;
    }
     div#storemapper.search_reset_enabled #storemapper-left{
         position:absolute;
         top:2%;
         max-height:94%;
         left:10px;
         z-index:1;
         height:auto !important;
         max-width: 300px;
    }
     div#storemapper #storemapper-right{
        width:100% !important
    }
     #storemapper #storemapper-list {
         padding: 0px;
    }
}
@media screen and (max-width:767px){
     #storemapper #storemapper-form #storemapper-zip {
         width: 100% !important;
         font-size: 12px;
         margin-right: 5px;
         height: auto;
    }
     #storemapper #storemapper-form .storemapper-dropdown-btn {
         color: #FFF;
         background: #FF9F19;
         font-size: 12px;
         padding: 5px;
         height: 35px;
         font-weight: bold;
         width: 100% !important;
    }
     #storemapper #storemapper-form #storemapper-go {
         color: #FFF;
         background: #FF9F19;
         font-size: 12px;
         padding: 5px;
         height: 35px;
         text-transform: none;
         width: 100% !important;
    }
     #storemapper.show_categories #storemapper-form .storemapper-categories {
         display: block;
    }
     #storemapper #storemapper-list li.storemapper-selected p.storemapper_directions a, #storemapper #storemapper-list li .storemapper_directions a {
         display: none !important;
    }
}

