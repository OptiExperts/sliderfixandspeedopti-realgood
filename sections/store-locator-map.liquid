     text-indent: -9999px;
     height: 15px;
     width: 15px;
     position: relative;
     height: 16px;
     width: 16px;
     background: url("{{'right-chevron_selected.png' | asset_url }}") 0 0 no-repeat #FFF;
     padding: 0px;
     border: 1px solid #FFF;
     background-size: cover;
     border-radius: 8px;
     display: block !important;
     z-index: 1;
     margin-top: 10px;
}
#storemapper .gm-style-iw-d {
     min-height: 130px;
     width: 180px;
}
 #storemapper #storemapper-list li .storemapper-address {
