}
 #storemapper .gm-style-iw {
     border-radius: 0px;
}
 #storemapper h4.storemapper-popup-name {
     color: #F8AD3E;
     font-size: 16px;
     font-weight: bold;
}
 #storemapper.show_icons .storemapper-popup-address {
     padding-left: 0px;
     font-size: 14px;
     line-height: 18px;
     margin: 10px 0px !important;
}
 #storemapper.show_icons .storemapper-popup-address svg {
     display: none;
}
