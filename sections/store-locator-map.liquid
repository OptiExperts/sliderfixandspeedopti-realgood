     background: url("{{'right-chevron.png' | asset_url }}") 0 0 no-repeat #FFF;
     padding: 0px;
     border: 1px solid #FFF;
     background-size: cover;
     border-radius: 8px;
     display: block !important;
     z-index: 1;
     margin-top: 10px;
}
#storemapper #storemapper-list li .storemapper_btn-wrap {
     position: absolute;
     top: 50px;
     right: 0px;
}
#storemapper #storemapper-list li p.storemapper-distance {
     position: relative !important;
     left: 0px;
     top: 0px;
}
#storemapper #storemapper-form .storemapper-dropdown-btn {
     color: #FFF;
     background: #FF9F19;
     font-size: 20px;
     padding: 10px 20px;
     height: auto;
     font-weight: bold;
