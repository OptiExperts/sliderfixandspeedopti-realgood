<div class="shop-page-wrapper">
  {%- assign products_limit = section.settings.products_limit_handle -%}
  {%- paginate collection.products by products_limit -%}
  
  {% if section.settings.shopbar_enable %}
  <div class="shop-page-header">
    <div class="{% if section.settings.container_width == 'container' %} container {% elsif section.settings.container_width == 'fluid' %} container wide {% endif %}">
      <div class="row align-items-center">
        {% include 'collection-sorting-2' %}
      </div>
    </div>
  </div>
  {% endif %}

  {% if section.settings.top_filter_menu %}
    {% include 'top_filter_widget' %}
  {% endif %}


  <div class="shop-page-content mt-0 mb-0 mt-sm-0 mb-sm-0">
    <div class="{% if section.settings.container_width == 'container' %} xcontainer {% elsif section.settings.container_width == 'fluid' %} {% if section.settings.sidebar_handle !=  'no_gutters' %} xcontainer xwide {%- else -%} container-fluid p-0 {%- endif -%} {%- endif -%}   ">
      <div class="row {% if section.settings.sidebar_handle == 'no_gutters' %} no-gutters {% endif %} {% if section.settings.sidebar_handle == 'right_sidebar_handle' %}flex-row-reverse{% endif %} ">

        
        {%- unless section.settings.sidebar_handle == 'no_gutters' -%}
        {%- unless section.settings.sidebar_handle == 'without_sidebar'  -%}
        <div class="col-lg-3 mb-md-80 mb-sm-80">
          {%- include 'collection-sidebar' -%}
        </div>
        {%- endunless -%}
        {%- endunless -%}
        
        
        
        <div class="{% if section.settings.sidebar_handle == 'without_sidebar' or section.settings.sidebar_handle == 'no_gutters' %}col-lg-12{% else %}col-lg-9{% endif %} col-12 ">
        {% if section.settings.shop_page_title_image_enable == true %}

          {% if section.settings.custom_collection_banner == "custom_banner" %}
          
          {% if section.settings.shop_page_enable_dual_bnr == true %}
              <div class="collection_banner dual--banner mb-20">
                
                {% if collection.metafields.arena.acf__enable_dual_banner == 'true' %}
                
                <div class="row">
                  <div class="col-lg-6 col-md-6 col-12 left--sec" style="background-image:url('{% if collection.metafields.arena.acf__dualb_l_img != blank %}{{ collection.metafields.arena.acf__dualb_l_img }}{% else %} https://via.placeholder.com/1080x425 {% endif %}')">
                    <div class="inner">
                      <div class="content">
                        {% if collection.metafields.arena.acf__dualb_l_head != blank %}
                        <h2>{{collection.metafields.arena.acf__dualb_l_head}}</h2>
                        {% endif %}
                        {% if collection.metafields.arena.acf__dualb_l_text != blank %}
                        <div class="text--content">
                          {{collection.metafields.arena.acf__dualb_l_text}}
                        </div>
                        {% endif %}
                        {% if collection.metafields.arena.acf__dualb_l_btn_label != blank %}
                        <div class="cta--btn">
                          <a href="{{collection.metafields.arena.acf__dualb_l_btn_link}}" {% if collection.metafields.arena.acf__dualb_l_link_newtab == true %}target="_blank"{% endif %}>{{collection.metafields.arena.acf__dualb_l_btn_label}}</a>
                        </div>
                        {% endif %}
                      </div>
                    </div>
                  </div>

                  <div class="col-lg-6 col-md-6 col-12 right--sec" style="background:{{collection.metafields.arena.acf__dualb_r_bg_color}}">
                    <div class="inner">
                      <div class="content">
                        {% if collection.metafields.arena.acf__dualb_r_head != blank %}
                        <h2>{{collection.metafields.arena.acf__dualb_r_head}}</h2>
                        {% endif %}
                        {% if collection.metafields.arena.acf__dualb_r_text != blank %}
                        <div class="text--content">
                          {{collection.metafields.arena.acf__dualb_r_text}}
                        </div>
                        {% endif %}
                        {% if collection.metafields.arena.acf__dualb_r_btn_label != blank %}
                        <div class="cta--btn">
                          <a href="{{collection.metafields.arena.acf__dualb_r_btn_link}}" {% if collection.metafields.arena.acf__dualb_r_link_newtab == true %}target="_blank"{% endif %}>{{collection.metafields.arena.acf__dualb_r_btn_label}}</a>
                        </div>
                        {% endif %}
                      </div>
                    </div>
                  </div>
                </div>
                
                
                {% comment %}
                
                {% else %}
                
                <div class="row">
                  <div class="col-lg-6 col-md-6 col-12 left--sec" style="background-image:url('{% if section.settings.shop_page_title_l_image != blank %}{{ section.settings.shop_page_title_l_image | img_url: '1080x425' }}{% else %} https://via.placeholder.com/1080x425 {% endif %}')">
					<div class="inner">
                      <div class="content">
                        {% if section.settings.shop_page_title_l_heading != blank %}
                        <h2>{{section.settings.shop_page_title_l_heading}}</h2>
                        {% endif %}
                        {% if section.settings.shop_page_title_l_content != blank %}
                        <div class="text--content">
                          {{section.settings.shop_page_title_l_content}}
                        </div>
                        {% endif %}
                        {% if section.settings.shop_page_title_l_btn_label != blank %}
                        <div class="cta--btn">
                          <a href="{{section.settings.shop_page_title_l_btn_link}}" {% if section.settings.shop_page_title_l_btn_link_blank == true %}target="_blank"{% endif %}>{{section.settings.shop_page_title_l_btn_label}}</a>
                        </div>
                        {% endif %}
                      </div>
                    </div>
                  </div>
                  
                  <div class="col-lg-6 col-md-6 col-12 right--sec" style="background:{{ section.settings.shop_page_title_r_bg_color }}">
                    <div class="inner">
                      <div class="content">
                        {% if section.settings.shop_page_title_r_heading != blank %}
                        <h2>{{section.settings.shop_page_title_r_heading}}</h2>
                        {% endif %}
                        {% if section.settings.shop_page_title_r_content != blank %}
                        <div class="text--content">
                          {{section.settings.shop_page_title_r_content}}
                        </div>
                        {% endif %}
                        {% if section.settings.shop_page_title_r_btn_label != blank %}
                        <div class="cta--btn">
                          <a href="{{section.settings.shop_page_title_r_btn_link}}" {% if section.settings.shop_page_title_r_btn_link_blank == true %}target="_blank"{% endif %}>{{section.settings.shop_page_title_r_btn_label}}</a>
                        </div>
                        {% endif %}
                      </div>
                    </div>
                  </div>
                </div>
                {% endcomment %}
                
                
                
                
              {% endif %}
          </div>
          
          {% else %}
          	{% if section.settings.shop_page_title_image != blank %}
            <div class="collection_banner mb-20">
              <img class="lazyload" data-src="{% if section.settings.shop_page_title_image != blank %}{{ section.settings.shop_page_title_image | img_url: 'master' }}{% else %} https://via.placeholder.com/1170x500 {% endif %}" alt="{{ section.settings.shop_page_title_image.alt }}">
            </div>
          	{% endif %}
          {% endif %}
          
          {%  elsif section.settings.custom_collection_banner == "each_collection_automate" %}
            {% if collection.image %}
              <div class="collection_banner mb-20">
                {% comment %}
                <img class="lazyload" data-src="{{ collection.image.src | collection_img_url: '1170x500' }}" alt="{{ collection.image.alt }}" />
                {% endcomment %}
                <img class="lazyload" data-src="{{ collection.image.src | collection_img_url: 'master' }}" alt="{{ collection.image.alt }}" />                
              </div>
            {% endif %}
          {% endif %}
        {% endif %}

          <div class="row">
            <div class="col-12">
              
              {% comment %}
              <div class="product_listing_header">
                <div id="product_desc">
                  <h1>Real Food You Feel Good About Eating</h1>
                  <h2>Low Carb | High Protein | Real Ingredients</h2>
                </div>
              </div>
              {% endcomment %}
              
              
              {% if collection.handle == 'all' %}
              <div class="product--cats-tab">
                <div class="product_listing_filter">
                  {% for collection1 in collections %}
                      {% if collection1.id == 132886560845 or collection1.id == 136606449741 or collection1.id == 159005474893 %}
                      {% elsif collection1.id  == 135477690445 %}
                        <a href="/collections/merch" class="collection_filter_button" data-order="100" style="display:none;" rel="{{ collection1.id }}">{{ collection1.title }}</a>
                      {% elsif collection1.id  == 136796864589 %}
                        <a href='#{{ collection1.handle }}' data-desc="{{ collection1.description | escape
}}" data-bg="{% if collection1.image %}{{ collection1.image | img_url: 'master' }}{% endif %}" class="collection_filter_button" data-order="1" rel="{{ collection1.id }}">{{ collection1.title }}</a>
                        <script type="text/javascript">
                          jQuery(document).ready(function(){
                            jQuery('#product_desc').html('{{ collection1.description}}');  
                          });
                        </script>
                      {% else %}
                        {% case collection1.id %}
                          {% when  137733177421 %}
                             {%- assign dataorder = 10 -%}
                          {% when  137369419853 %}
                             {%- assign dataorder = 20 -%}
                          {% when  137397403725 %}
                             {%- assign dataorder = 30 -%}
                          {% when  137394094157 %}
                             {%- assign dataorder = 40 -%}
                          {% when  137393340493 %}
                             {%- assign dataorder = 50 -%}
                          {% when  135477690445 %}
                             {%- assign dataorder = 60 -%}
                          {% when  159489327181 %}
                             {%- assign dataorder = 101 -%}
                          {% else %}
                             {%- assign dataorder = 99 -%}
                        {% endcase %}
                        <a href='#{{ collection1.handle }}' data-desc="{{ collection1.description | escape
}}" data-bg="{% if collection1.image %}{{ collection1.image | img_url: 'master' }}{% endif %}" class="collection_filter_button" data-order="{{ dataorder }}" rel="{{ collection1.id }}">{{ collection1.title }}</a>
                      <img src="{% if collection1.image %}{{ collection1.image | img_url: 'master' }}{% endif %}" class="hide" style="display: none !important;" />
                      {% endif %}
                  {% endfor %}
                </div>
              </div>
              
               {% else %}
              <div class="product--cats-tab">
              	<div class="product_listing_filter">
                  {% for collection1 in collections %}
                      {% if collection1.id == 132886560845 or collection1.id == 136606449741 or collection1.id == 159005474893 or collection1.id == 396785058047 or collection1.id == 159489327181 %}
                      {% elsif collection1.id  == 135477690445 %}
                        <a href="/collections/merch" class="collection_filter_button" data-order="100" style="display:none;" rel="{{ collection1.id }}">{{ collection1.title }}</a>
                      {% elsif collection1.id  == 136796864589 %}
                        <a href='{{ collections[collection1.handle].url }}' data-desc="{{ collection1.description | escape
}}" data-bg="{% if collection1.image %}{{ collection1.image | img_url: 'master' }}{% endif %}" class="collection_filter_button {% if request.path contains collection1.handle %} active{% endif %}" data-order="1" rel="{{ collection1.id }}">{{ collection1.title }}</a>
                        <script type="text/javascript">
                          jQuery(document).ready(function(){
                            jQuery('#product_desc').html('{{ collection1.description}}');  
                          });
                        </script>
                      {% else %}
                        {% case collection1.id %}
                          {% when  137733177421 %}
                             {%- assign dataorder = 10 -%}
                          {% when  137369419853 %}
                             {%- assign dataorder = 20 -%}
                          {% when  395338547455 %}
                          	{%- assign dataorder = 25 -%}
                          {% when  137394094157 %}
                             {%- assign dataorder = 30 -%}
                          {% when  137397403725 %}
                             {%- assign dataorder = 40 -%}
                          {% when  137393340493 %}
                             {%- assign dataorder = 50 -%}
                          {% when  135477690445 %}
                             {%- assign dataorder = 60 -%}
                          {% when  159489327181 %}
                             {%- assign dataorder = 101 -%}
                          {% else %}
                             {%- assign dataorder = 99 -%}
                        {% endcase %}
                        <a href='{{ collections[collection1.handle].url }}' data-desc="{{ collection1.description | escape
}}" data-bg="{% if collection1.image %}{{ collection1.image | img_url: 'master' }}{% endif %}" class="collection_filter_button {% if request.path contains collection1.handle %} active{% endif %}" data-order="{{ dataorder }}" rel="{{ collection1.id }}">{{ collection1.title }}</a>
                      {% endif %}
                  {% endfor %}
                </div>
              </div>
              
               {% endif; %} 
              
            </div>
          </div>
          <div class="row {% if section.settings.sidebar_handle == 'no_gutters' %} no-gutters {% endif %} product-isotope shop-product-wrap three-column">
            {%- for product in collection.products -%}
{%- comment -%} Wholesale Gorilla: do not delete unless app is uninstalled{%- endcomment -%}
{%- assign wsgTempProd = product -%}{%- assign wsgExclude = false -%}{%- if forloop.first == true -%}{%- assign wsgActive = false -%}{%- assign wsgActiveCustomer = false -%}
{%- include 'wsg-status' wsgStatusAction: 'init' | strip -%}{%- endif -%}{%- if wsgActive -%}{%- include 'wsg-status' wsgStatusAction: 'col-item' | strip -%}{%- endif -%}{%- if wsgExclude -%} {%- continue -%} {%- endif -%}
{%- comment -%} end WSG {%- endcomment -%}
            
              {% if section.settings.product_style == 'style1' %}
                {%- include 'product-grid-item' -%}
              {% elsif section.settings.product_style == 'style2' %}
                {%- include 'product-grid-item' -%}
              {% elsif section.settings.product_style == 'style3' %}
                {%- include 'product-grid-item' -%}
              {% endif %}


            {% endfor %}
          </div>

          <div class="row">
            <div class="col-lg-12 text-center mt-30">
              {%- include 'pagination' -%}
            </div>
          </div>

        </div>
      </div>
    </div>
  </div>

  {%- endpaginate -%}
</div>

