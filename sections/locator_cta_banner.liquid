{% schema %}
  {
    "name": "Call To Action",
    "settings": [		
		{
		  "type": "header",
          "content": "Section Enable"
        },
		{
          "type": "checkbox",
          "id": "section_enable",
          "label": "Section Enable",  
          "default": true
      	},
		{
         "type": "header",
         "content": "Call To Action With Banner"
        },
		{
		  "type": "image_picker",
          "id": "section_bg_img",
          "label": "Background Image",
          "info": "Section Background Image"
        },
		{
		  "type": "image_picker",
          "id": "section_bg_img_mob",
          "label": "Background Image (Mobile)",
          "info": "Section Background Image (Mobile)"
        },
		{
		  "type": "image_picker",
          "id": "text_heading_img",
          "label": "Image",
          "info": "Heading of text content"
        },
		{
          "type": "textarea",
          "id": "cta_text_content",
          "label": "Text Content",
          "default": "Text Content"
        },
		{
          "type": "text",
          "id": "call_to_action_btn",
          "label": "Call To Action Btn",
          "default": "Contact Us"
        },
		{
          "type": "url",
          "id": "call_to_action_btn_link",
          "label": "URL"
        },
		{
          "type": "text",
          "id": "cta2_btn",
          "label": "Call To Action 2 Btn",
          "default": "Contact Us"
        },
		{
          "type": "url",
          "id": "cta2_btn_link",
          "label": "CTA 2 URL"
        },
		{
          "type": "text",
          "id": "cta3_btn",
          "label": "Call To Action 3 Btn (Left)",
          "default": "Shop Now"
        },
		{
          "type": "url",
          "id": "cta3_btn_link",
          "label": "CTA 3 URL"
        }
	],
	"blocks": [
		{
          "type": "call_to_action_style",
          "name": "Section Style",
		  "limit": 1,
