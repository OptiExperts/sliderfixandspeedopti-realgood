{% comment %}
{% if section.settings.section_enable %}
<div class="cta-area pt-50 pb-50" id="section-{{ section.id }}">
  <div class="container">
    <div class="row">
      <div class="col-lg-12">
        <div class="cta-content">
          {% if section.settings.cta_text_content != blank %}
          <div class="cta_text_content">
            {% if section.settings.text_heading_img != blank %}
            	<img src="{{ section.settings.text_heading_img | img_url: 'master' }}" alt="" class="cta_text_heading_img">
            {% endif %}
            {{ section.settings.cta_text_content }}
          </div>
          {% endif %}
          {% if section.settings.call_to_action_btn != blank %}
          <div class="cta-button">
            <a class="lezada-button lezada-button--medium lezada-cta-button" href="{{ section.settings.call_to_action_btn_link }}" target="_blank">{{ section.settings.call_to_action_btn }}</a>
          </div>
          {% endif %}
        </div>
      </div>
    </div>
  </div>
</div>
{% endif %}
{% endcomment %}


{% if section.settings.section_enable %}
<div class="cta-area" id="section-{{ section.id }}">
  {% if section.settings.section_bg_img != blank %}
  <div class="bnrimg">
    {% comment %}
    <img data-src="{{ section.settings.section_bg_img | img_url: 'master' }}" alt="" class="section_bg_img lazy">
    <img data-src="{{ section.settings.section_bg_img_mob | img_url: 'master' }}" alt="" class="section_bg_img mobile lazy">
    {% endcomment %}
    
    <img data-src="{{ section.settings.section_bg_img | img_url: '1000x505' }}" alt="" class="section_bg_img lazy">
    <img data-src="{{ section.settings.section_bg_img_mob | img_url: '768x385' }}" alt="" class="section_bg_img mobile lazy"> 
   
    {% if section.settings.cta3_btn != blank %}
    <div class="cta-button cta--btn-3 right--sidebtn">
      <a class="lezada-button lezada-button--medium lezada-cta-button" href="{{ section.settings.cta3_btn_link }}" target="_blank">{{ section.settings.cta3_btn }}</a>
    </div>
    {% endif %}
    
    
  </div>
  {% endif %}
  
  <div class="cta-content">
    <div class="container">
      <div class="row">
      	<div class="col cta_inner">        
          {% if section.settings.cta_text_content != blank %}
          <div class="cta_text_content">
            {% if section.settings.text_heading_img != blank %}
            	<img data-src="{{ section.settings.text_heading_img | img_url: 'master' }}" alt="" class="cta_text_heading_img lazy">
            {% endif %}
            {{ section.settings.cta_text_content }}
          </div>
          {% endif %}
          {% if section.settings.cta3_btn != blank %}
          <div class="cta-button cta--btn-3 mobile">
            <a class="lezada-button lezada-button--medium lezada-cta-button" href="{{ section.settings.cta3_btn_link }}" target="_blank">{{ section.settings.cta3_btn }}</a>
          </div>
          {% endif %}
          {% if section.settings.call_to_action_btn != blank %}
          <div class="cta-button">
            <a class="lezada-button lezada-button--medium lezada-cta-button" href="{{ section.settings.call_to_action_btn_link }}" target="_blank">{{ section.settings.call_to_action_btn }}</a>
          </div>
          {% endif %}
          {% if section.settings.cta2_btn != blank %}
          <div class="cta-button cta--btn-2">
            <a class="lezada-button lezada-button--medium lezada-cta-button" href="{{ section.settings.cta2_btn_link }}" target="_blank">{{ section.settings.cta2_btn }}</a>
          </div>
          {% endif %}
        </div>
      </div>
    </div>
  </div>
</div>
{% endif %}


